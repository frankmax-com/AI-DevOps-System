# Environment Configuration
LOG_LEVEL=INFO
PORT=8080
AI_PROVIDER_CONFIG_PATH=./config/ai_providers_config.json

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_DB=0

# AI Provider API Keys (Optional - for paid tiers)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
HUGGINGFACE_API_KEY=
GITHUB_TOKEN=

# Security
JWT_SECRET_KEY=your-secret-key-here-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Rate Limiting
GLOBAL_RATE_LIMIT=1000
RATE_LIMIT_WINDOW_SECONDS=3600

# Monitoring
ENABLE_PROMETHEUS=true
PROMETHEUS_PORT=8081
ENABLE_STRUCTURED_LOGGING=true

# Cache Settings
CACHE_TTL_HOURS=24
MAX_CACHE_SIZE=1000

# Provider Timeouts (seconds)
PROVIDER_TIMEOUT=30
PROVIDER_RETRY_ATTEMPTS=3
PROVIDER_RETRY_DELAY=1

# Cost Tracking
ENABLE_COST_TRACKING=true
COST_ALERT_THRESHOLD=100.00

# Development Settings
DEBUG=false
RELOAD=false
